{
    "name": "hexolus/user",
    "description": "User Module by Hexolus",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Meta Sanjaya",
            "email": "metasanjaya@gmail.com"
        }
    ],
    "require": {
        "hexolus/uuid": "1.x-dev",
        "spatie/laravel-permission": "^3.18"
    },
    "autoload": {
        "psr-4": {
            "Hexolus\\User\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Hexolus\\User\\UserServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hexolus/uuid.git"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^6.7"
    }
}
